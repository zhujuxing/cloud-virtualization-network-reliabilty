	Author = {Matos, R. D. S. and Maciel, P. R. M. and Machida, F. and Kim, Dong Seong and Trivedi, K. S.},
	Title = {Sensitivity Analysis of Server Virtualized System Availability},
	Journal = {IEEE transactions on reliability},
